{
    "manifest_version": 3,
    "name": "Remove Google AI Overviews",
    "version": "1.7.0",
    "description": "Hide all annoying Google AI overviews.",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
          "matches": ["*://*.google.com/*"],
          "js": ["js/website.js"]
        }
    ],
     "icons": {
        "16": "img/16.png",
        "32": "img/32.png",
        "48": "img/48.png",
        "64": "img/64.png",
        "96": "img/96.png",
        "128": "img/128.png"
     }
  }